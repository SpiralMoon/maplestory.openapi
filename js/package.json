{
  "name": "maplestory-openapi",
  "version": "2.7.1",
  "description": "This JavaScript library enables the use of the MapleStory OpenAPI of Nexon.",
  "keywords": [
    "maplestory",
    "maplestory openapi",
    "maplestory api",
    "cube",
    "nexon",
    "nexon developers",
    "nexon openapi"
  ],
  "homepage": "https://github.com/SpiralMoon/maplestory.openapi",
  "bugs": {
    "url": "https://github.com/SpiralMoon/maplestory.openapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpiralMoon/maplestory.openapi.git"
  },
  "license": "MIT",
  "author": "SpiralMoon",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rollup -c",
    "js_sample": "node sample/js_sample.js",
    "lint": "eslint ./src/** --fix",
    "format": "prettier --write ./**/*.{ts,js}",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "ts_sample": "ts-node sample/ts_sample.ts",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@types/jest": "^29.5.12",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "rollup": "2.66.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.1",
    "tsm": "2.2.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=12"
  },
  "email": "dev.spiralmoon@gmail.com",
  "umd:name": "maplestory-openapi"
}
