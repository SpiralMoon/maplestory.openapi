{
  "name": "maplestory-openapi",
  "version": "1.0.0",
  "keywords": [
    "maplestory",
    "maplestory openapi",
    "maplestory api",
    "cube",
    "nexon",
    "nexon developers"
  ],
  "description": "This JavaScript library enables the use of the MapleStory OpenAPI provided by Nexon.",
  "homepage": "https://github.com/SpiralMoon/maplestory.openapi",
  "bugs": {
    "url": "https://github.com/SpiralMoon/maplestory.openapi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpiralMoon/maplestory.openapi.git"
  },
  "license": "MIT",
  "author": "SpiralMoon",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rollup -c",
    "js_sample": "node sample/js_sample.js",
    "prepublishOnly": "npm run build",
    "test": "uvu -r tsm test",
    "ts_sample": "ts-node sample/ts_sample.ts",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "rollup": "2.66.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.27.1",
    "ts-node": "^10.9.1",
    "tsm": "2.2.1",
    "typescript": "^5.2.2",
    "uvu": "0.5.3"
  },
  "engines": {
    "node": ">=12"
  },
  "email": "dev.spiralmoon@gmail.com",
  "umd:name": "maplestory-openapi"
}
